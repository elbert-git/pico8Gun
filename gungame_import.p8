function create_bomb(_x, _y)
	if bomb.active == false and player.bombs > 0 then
		-- deplete one bomb
		player.bombs-=1
		-- create bomb
		bomb.pos.x = _x
		bomb.pos.y = _y
		bomb.active = true
		bomb.lifespan=1
	end
end
function bomb_update()
	-- just delay lifespan
	if bomb.active then
		bomb.lifespan -= clock.delta
		if bomb.lifespan < 0 then
			-- if lifespan runs out
			-- bomb boom
			bomb.active = false
			bomb_boom()
		end
	end
end
