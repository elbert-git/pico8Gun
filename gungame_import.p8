-- on start create intial bullets
for i=1, 20 do
	bullets[#bullets+1]={
		pos={x=-9000, y=-9000},
		dir={x=0, y=0},
		active=false
	}
end

function create_bullet(_x, _y, _dir)
	-- get bullet
	local curr_b = bullets[b_curr_i]
	-- set bullet initial vars
	curr_b.active = true
	curr_b.pos = {x=_x, y=_y}
	curr_b.dir = _dir
	-- iterate bullet index
	b_curr_i = b_curr_i + 1
	if b_curr_i > #bullets then
		b_curr_i = 1
	end
end

function bullet_update()
	-- bullet downtime 
	b_down_time -= clock.delta
	-- bullet update
	for i=1, #bullets do
		local b = bullets[i]
		if b.active then
			-- movement
			b.pos.x += b.dir.x * b_spd;
			b.pos.y += b.dir.y * b_spd;
			--collision
			for ei=1, #enemies do
			 local e = enemies[ei]
			 if obj_collide(b, e)  and e.active then
			 	b_cull_i=i
			 end
			end		
		end
	end
end

function bullet_draw()
	for i=1, #bullets do
		local b = bullets[i]
		if b.active then
			spr(93, b.pos.x, b.pos.y)	
		end
	end
end


